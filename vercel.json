{
  "cleanUrls": true,
  "functions": {
    "*.js": {
      "runtime": "vercel-node@latest"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://crm.zoho.com https://crmplus.zoho.com https://*.zohocrm.com;"
        }
      ]
    }
  ]
}
